{%
  set body_classes = [
    logged_in ? 'user-logged-in',
    not root_path ? 'path-frontpage' : 'path-' ~ root_path|clean_class,
    node_type ? 'page-node-type-' ~ node_type|clean_class,
    db_offline ? 'db-offline',
    theme.settings.navbar_position ? 'navbar-is-' ~ theme.settings.navbar_position,
    theme.has_glyphicons ? 'has-glyphicons',
  ]
%}
<!DOCTYPE html>
<html {{ html_attributes }}>
  <head>
    <head-placeholder token="{{ placeholder_token|raw }}">
    <title>{{ head_title|safe_join(' | ') }}</title>
    <css-placeholder token="{{ placeholder_token|raw }}">
    <js-placeholder token="{{ placeholder_token|raw }}">
  </head>
  <body{{ attributes.addClass(body_classes) }}>
    <a href="#main-content" class="visually-hidden focusable skip-link">
      {{ 'Skip to main content'|t }}
    </a>
    <nav>
        {{page.navigation}}
			<div id="tc-navbar" class="navbar">
				<a href="#"><i class="fas fa-arrow-left"></i></a>
				<a href="#"><img id="home-button" src="tc_logo2.svg"/></a>
				<a href="search.html"><i class="fas fa-search"></i></a>
			</div>
		</nav>

		<div id="logo-tc">
        {{page.header}}
        <!--<img id="logo-img" src="tk_logo.svg">-->.
        </div>

		<div id="wrapper">
        {{page.content}}
			<a href="index_tk.html" class="card-top no-select">
				<p>Gå till T-konventet</p>
			</a>

			<div id="cards-wrapper">
				<div class="card-container">
					<a href="template_evenemang.html" class="card card-blue no-select" style="background-image: url(img/2.png)">
						<p>Evenemang</p>
					</a>

					<a href="template_gray.html" class="card card-gray no-select" style="background-image: url(img/4.jpg)">
						<p>För företag</p>
					</a>

					<a href="template_orange.html" class="card card-orange no-select" style="background-image: url(img/elevprojekt.jpg)">
						<p>
							Plugga till<br />
							gymnasieingenjör
						</p>
					</a>

					<a href="template_utmärkelser.html" class="card card-blue no-select" style="background-image: url(img/9.png)">
						<p>Utmärkelser</p>
					</a>
				</div>

				<div class="card-container">
					<a href="spel/arcade.html" class="card card-orange no-select" style="background-image: url(img/3.png)">
						<p>TC-Arcade</p>
					</a>

					<a href="template_program.html" class="card card-blue no-select" style="background-image: url(img/5.png)">
						<p>Program &amp;<br />inriktningar</p>
					</a>

					<a href="template_uf.html" class="card card-gray no-select" style="background-image: url(img/6.jpg)">
						<p>Ung<br />företagsamhet</p>
					</a>

					<a href="about.html" class="card card-orange no-select" style="background-image: url(img/7.png)">
						<p>Om oss</p>
					</a>
				</div>
			</div>

			<p class="space-top-1 body-text text-centered">
				Följ oss på våra sociala medier<br /><span>@tcstenungsund</span>
			</p>

			<div class="social-flex">
				<a
					class="space-bottom-1 social-icon"
					onclick="window.open('https://www.facebook.com/tcstenungsund/', '_system'); return false;"
					><i class="fab fa-facebook"></i
				></a>
				<a
					class="space-bottom-1 social-icon"
					onclick="window.open('https://www.instagram.com/tcstenungsund/', '_system'); return false;"
					><i class="fab fa-instagram"></i
				></a>
				<a
					class="space-bottom-1 social-icon"
					onclick="window.open('https://twitter.com/tcstenungsund?lang=sv', '_system'); return false;"
					><i class="fab fa-twitter"></i
				></a>
			</div>
		</div>
    <js-bottom-placeholder token="{{ placeholder_token|raw }}">
  </body>
</html>